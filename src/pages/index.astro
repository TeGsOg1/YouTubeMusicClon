---
import Layout from "../layouts/Layout.astro";
import Music from "../components/Music.astro";
---

<Layout title="YouTube Music Clon">
  <h1 class="block text-4xl font-bold text-white">YouTube Music Clon</h1>
  <div class="flex flex-row overflow-auto" id="canciones">
    <Music
      title="Tu tanta falta de querer (En Vivo)"
      artist="Mon Laferte"
      image="https://cdns-images.dzcdn.net/images/cover/ba3b903bf0abb562a47c4b8fef06d1de/500x500.jpg"
    />
    <Music
      title="Un Osito de Peluche de Taiwán"
      artist="Los Auténticos Decadentes"
      image="data:image/jpeg;base64,"
    />
    <Music
      title="The Nights"
      artist="Avicii"
      image="data:image/jpeg;base64,"
    />
    <Music
      title="Unitl I Found You (Piano Version)"
      artist="Stephen Sanchez"
      image="data:image/jpeg;base64,"
    />
  </div>
</Layout>

<script>
  const options = {
	method: 'GET',
	 headers: {
	  'X-RapidAPI-Key': '02ea4c2d99msh9436a7e1b8cf5e1p1aba42jsneeb82c4818fe',
	  'X-RapidAPI-Host': 'spotify23.p.rapidapi.com'
	}
  };
  
  function apiCall(done: (data: any) => void) {
    const resultados = fetch('https://spotify23.p.rapidapi.com/search/?q=%3CREQUIRED%3E&type=multi&offset=0&limit=10&numberOfTopResults=5', options);
    resultados
      .then(res => res.json())
      .then(data => {
        done(data);
      });
  }

  apiCall((data: any) => {
    data.albums.forEach((results: any) => {
      const article = document.createRange().createContextualFragment(`
      <article class="flex flex-col w-60 h-auto p-2">
         <img
           class="w-full"
           src="${ results.images[0].url }"
           alt=""
          />
         <h1 class="text-white justify-start text-xl">
          ${ results.name }
          </h1>
         <p class="text-gray-400 opa">
          ${ results.artists[0].name }
          </p>
     </article>
      `);
      const music = document.querySelector('#canciones');
      music?.append(article)
    });
  });
</script>